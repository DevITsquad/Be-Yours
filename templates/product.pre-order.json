          "type": "rating",
          "settings": {
          }
        },
        "countdown": {
          "type": "countdown",
          "settings": {
            "heading": "Hurry up ! Sale end in:",
            "number_size": "compact",
            "colors_heading": "#da3f3f",
            "colors_text": "#da3f3f",
            "colors_background": "#ffeeee"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
