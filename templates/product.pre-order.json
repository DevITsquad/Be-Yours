    "main": {
      "type": "main-product",
      "blocks": {
        "subtitle": {
          "type": "text",
          "settings": {
            "icon": "none",
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "title": {
          "type": "title",
          "settings": {
            "title_size": "h2"
          }
        },
