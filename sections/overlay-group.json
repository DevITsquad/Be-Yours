        "mobile-dock-1": {
          "type": "menu",
          "settings": {
            "custom_icon": "",
            "title": "Menu"
          }
        },
        "mobile-dock-2": {
          "type": "search",
          "settings": {
            "custom_icon": "",
            "title": "Search"
          }
        },
        "mobile-dock-3": {
          "type": "link",
          "settings": {
            "custom_icon": "",
            "title": "Shop",
            "link": "shopify:\/\/collections\/all"
          }
        },
        "mobile-dock-4": {
          "type": "cart",
          "settings": {
            "custom_icon": "",
            "title": "Cart"
          }
        },
        "mobile-dock-5": {
          "type": "account",
          "disabled": true,
          "settings": {
            "custom_icon": "",
            "title": "Account"
          }
        }
      },
      "block_order": [
        "mobile-dock-0",
        "mobile-dock-1",
        "mobile-dock-2",
        "mobile-dock-3",
        "mobile-dock-4",
        "mobile-dock-5"
      ],
      "settings": {
        "use_custom_colors": false,
        "colors_text": "#1a1b18",
        "colors_background": "#ffffff",
        "gradient_background": "",
        "padding_top": 12,
        "padding_bottom": 12
      }
